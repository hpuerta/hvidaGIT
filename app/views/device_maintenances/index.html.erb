<div class="page-header">
  <%= link_to new_device_maintenance_path, class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Device maintenance
  <% end %>
  <h1>Listing device_maintenances</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Fechamantenmiento</th>
            <th>Correctivo</th>
            <th>Prioridad</th>
            <th>Leido</th>
            <th>Completado</th>
            <th>Realizadopor</th>
            <th>Descripcion</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @device_maintenances) do |device_maintenance| %>
            <td><%= device_maintenance.fechaMantenmiento %></td>
            <td><%= device_maintenance.correctivo %></td>
            <td><%= device_maintenance.prioridad %></td>
            <td><%= device_maintenance.leido %></td>
            <td><%= device_maintenance.completado %></td>
            <td><%= device_maintenance.realizadoPor %></td>
            <td><%= device_maintenance.descripcion %></td>
            <td><%= link_to 'Show', device_maintenance %></td>
        <td><%= link_to 'Edit', edit_device_maintenance_path(device_maintenance) %></td>
        <td><%= link_to 'Destroy', device_maintenance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
