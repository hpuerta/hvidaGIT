<div class="page-header">
  <%= link_to programs_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_program_path(@program), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show program</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nombre:</dt>
  <dd><%= @program.nombre %></dd>

  <dt>Version:</dt>
  <dd><%= @program.version %></dd>

  <dt>Licencias:</dt>
  <dd><%= @program.licencias %></dd>

</dl>
