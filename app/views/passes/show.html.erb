<div class="page-header">
  <%= link_to passes_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_pass_path(@pass), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show pass</h1>
</div>

<dl class="dl-horizontal">
  <dt>Programa:</dt>
  <dd><%= @pass.programa %></dd>

  <dt>Usuario:</dt>
  <dd><%= @pass.usuario %></dd>

  <dt>Clave:</dt>
  <dd><%= @pass.clave %></dd>

</dl>
